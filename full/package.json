{
  "name": "svelte-leaflet-demo",
  "author": "Daniel Imfeld",
  "license": "MIT",
  "version": "0.0.2",
  "scripts": {
    "clean": "rimraf public",
    "rollup": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "copy-snowpack-templates": "copyfiles -u 1 snowpack/* public",
    "build": "NODE_ENV=production run-s clean rollup",
    "dev": "npm-run-all clean copy-snowpack-templates start:snowpack",
    "dev:rollup": "NODE_ENV=development npm-run-all clean --parallel start:dev rollup:watch",
    "start": "sirv public --single -H 0.0.0.0",
    "start:dev": "NODE_ENV=development sirv public --single --dev -H 0.0.0.0",
    "start:snowpack": "NODE_ENV=development snowpack dev"
  },
  "dependencies": {
    "@elfalem/leaflet-curve": "^0.6.0",
    "d3-scale": "^3.2.3",
    "just-flush": "^1.1.0",
    "leaflet": "~1.6",
    "sirv-cli": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.10.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@snowpack/app-scripts-svelte": "^1.9.2",
    "@snowpack/plugin-postcss": "^1.0.7",
    "@tailwindcss/ui": "^0.6.2",
    "@types/leaflet": "^1.5.19",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.0.3",
    "copyfiles": "^2.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "lodash.template": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "prettier": "^2",
    "prettier-plugin-svelte": "^1",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.0.0",
    "rollup-plugin-svelte": "^6",
    "rollup-plugin-terser": "^7.0.0",
    "snowpack": "^2.16.1",
    "svelte": "^3.29.4",
    "svelte-preprocess": "^4.5.2",
    "tailwindcss": "^1.9.0",
    "typescript": "^4.0.5"
  }
}
